{
  "name": "jupyter-offlinenotebook",
  "version": "0.1.0",
  "description": "JupyterLab offline-notebook extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/manics/jupyter-offlinenotebook",
  "bugs": {
    "url": "https://github.com/manics/jupyter-offlinenotebook/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Simon Li",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/manics/jupyter-offlinenotebook.git"
  },
  "scripts": {
    "build": "jlpm run build:all",
    "build:all": "jlpm run build:lib && jlpm run build:webpack",
    "build:lib": "tsc",
    "build:webpack": "webpack",
    "clean": "jlpm run clean:all",
    "clean:all": "jlpm run clean:lib && jlpm run clean:webpack",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:webpack": "rimraf jupyter_offlinenotebook/static/jslib",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "pip install -e . && jupyter labextension develop --overwrite .",
    "link": "jupyter labextension link . --no-build",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/docregistry": "^2.0.2",
    "@jupyterlab/notebook": "^2.0.2",
    "@lumino/disposable": "^1.3.5",
    "@types/jquery": "^3",
    "dexie": "^3.0.2",
    "jquery": "^3"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-beta.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "~4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter-offlinenotebook"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyter_offlinenotebook/static"
  }
}
